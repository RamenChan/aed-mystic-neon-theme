{
	"name": "AED Cyberpunk Neon",
	"colors": {
		"editor.background": "#050505",
		"editor.foreground": "#d0d0d0",
		"editorCursor.foreground": "#39ff14",
		"editor.lineHighlightBackground": "#121212",
		"editor.selectionBackground": "#ff00ff55",
		"editor.selectionHighlightBackground": "#39ff1422",
		"editor.findMatchBackground": "#ff007755",
		"editor.findMatchBorder": "#ff0077",
		"activityBar.background": "#080808",
		"activityBar.foreground": "#39ff14",
		"activityBarBadge.background": "#ff0077",
		"sideBar.background": "#101010",
		"sideBar.foreground": "#bbbbbb",
		"sideBarTitle.foreground": "#ffffff",
		"statusBar.background": "#080808",
		"statusBar.foreground": "#39ff14",
		"terminal.background": "#0a0a0a",
		"terminal.foreground": "#39ff14",
		"terminalCursor.foreground": "#ff0077",
		"breadcrumb.foreground": "#ff00ff",
		"breadcrumb.activeSelectionForeground": "#ff0077",
		"editorGroup.border": "#222222",
		"editorGutter.background": "#101010",
		"editorIndentGuide.background": "#222222",
		"editorIndentGuide.activeBackground": "#39ff14",
		"list.activeSelectionBackground": "#39ff1455",
		"list.hoverBackground": "#222222",
		"tab.activeBackground": "#121212",
		"tab.inactiveBackground": "#080808",
		"tab.border": "#222222"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#757575"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#39ff14"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#ff0077"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#ff00ff"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#bb86fc"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#ffcc00"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#39ff14"
			}
		},
		{
			"name": "JSON Keys",
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#ff0077"
			}
		}
	]
}
